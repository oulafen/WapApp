%body
  =render 'admins/jarvis_admin_css'
  #page-content.width_100(onkeypress="enter_form(event)" style="margin:0" )
    %h1 管理员用户管理
    %a.cursor-pointer(href="/admins/all_admins" )
      管理员用户管理>>
    修改管理员信息
    %br
    =render 'admins/modify_admin_info_form'
    .row
      .col-xs-5
      .col-xs-2
        %button.btn-local_2#submit(onclick="modify_admin_info(#{@modify_admin[:id]},'admin')" )
          确定
      .col-xs-2
        %button.btn-local_1(onclick="back_to_all_admins('admin')" )
          取消
